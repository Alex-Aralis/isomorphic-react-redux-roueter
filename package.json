{
  "name": "isomorphic-react-redux-router",
  "version": "0.0.0",
  "description": "This is a basic isomorphic react server with using router and redux.",
  "main": "build/lib/node_modules/server.js",
  "scripts": {
    "dev": "npm run compiler && npm run watch",
    "clean": "mkdir -p build && rm -r build",
    "compiler": "concurrently 'npm run build-views' 'npm run build-lib'",
    "watch": "concurrently -p '{command}' -l 10 --kill-others 'npm run watch-src' 'npm run watch-lib' 'npm run watch-views' 'npm run server'",
    "build": "npm run clean && npm run compiler && npm run webpack",
    "build-views": "mkdir -p build && cp -r views/ build/",
    "build-lib": "mkdir -p build/lib/node_modules && babel -d build/lib/node_modules src",
    "webpack": "webpack --progress --config webpack.config.dev.js",
    "watch-src": "babel -wd build/lib/node_modules src",
    "watch-views": "cpx --watch 'views/**/*' build/views",
    "server": "cd build && nodemon --on-change-only --config ../nodemon.json lib/node_modules/server",
    "watch-lib": "webpack --watch --colors --config webpack.config.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Alex-Aralis/isomorphic-react-redux-roueter.git"
  },
  "author": "Alex Aralis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex-Aralis/isomorphic-react-redux-roueter/issues"
  },
  "homepage": "https://github.com/Alex-Aralis/isomorphic-react-redux-roueter#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "express": "^4.14.0",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "jade": "^1.11.0",
    "lodash": "^4.15.0",
    "nodemon": "^1.10.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "concurrently": "^2.2.0",
    "eslint": "^3.3.1",
    "source-map-support": "^0.4.2",
    "watch": "^0.19.2"
  }
}
