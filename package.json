{
  "name": "isomorphic-react-redux-router",
  "version": "0.0.0",
  "description": "This is a basic isomorphic react server with using router and redux.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch-src & npm run watch-lib & npm run server",
    "watch-src": "mkdir -p build/lib && babel -wd build/lib src",
    "server": "cd build && nodemon lib/server",
    "watch-lib": "webpack --watch --progress --colors --config webpack.config.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Alex-Aralis/isomorphic-react-redux-roueter.git"
  },
  "author": "Alex Aralis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex-Aralis/isomorphic-react-redux-roueter/issues"
  },
  "homepage": "https://github.com/Alex-Aralis/isomorphic-react-redux-roueter#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "express": "^4.14.0",
    "nodemon": "^1.10.2",
    "react": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.3.1",
    "watch": "^0.19.2"
  }
}
